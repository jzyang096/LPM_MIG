	<!-- 客户主办客户经理异常报表 -->
	<LPM-RPT-1320>
		select cb.OrgID,
		cb.UserID,
		ci.MFCustomerID,
		ci.CustomerID,
		ci.CustomerName,
		getItemName('CustomerType', ci.CustomerType)
		from
		CUSTOMER_INFO_Mig ci
		left join CUSTOMER_BELONG_Mig cb
		on ci.CustomerID =
		cb.CustomerID
		where cb.UserID is null
		or cb.OrgID is null
	</LPM-RPT-1320>
