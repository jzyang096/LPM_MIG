	<!-- 客户(个人)信息明细表 -->
	<LPM-RPT-2060>
		select cb.OrgID,
		getOrgName(cb.OrgID),
		cb.UserID,
		ci.MFCustomerID,
		ci.CustomerID,
		ci.CustomerName,
		getItemName('CertType',ci.CertType),
		ci.CertID,
		getItemName('Sex',ii.Sex),
		ii.BirthDay,
